{
  "name": "shiok",
  "version": "1.0.0-semantic-release",
  "description": "shiok",
  "homepage": "https://github.com/bumbleblym/shiok#readme",
  "bugs": {
    "url": "https://github.com/bumbleblym/shiok/issues"
  },
  "license": "UNLICENSED",
  "author": "bumbleblym <bumbleblym@gmail.com> (https://github.com/bumbleblym)",
  "repository": {
    "type": "git",
    "url": "https://github.com/bumbleblym/shiok.git"
  },
  "scripts": {
    "lint": "esw",
    "lint:fix": "npm run lint -s -- --fix",
    "lint:watch": "npm run lint -s -- -w",
    "test": "mocha \"src/**/*.spec.js\"",
    "test:watch": "npm t -- -w",
    "cover": "npm run lint -s && nyc npm t && nyc report --reporter=lcov",
    "check-coverage": "nyc check-coverage",
    "report-coverage": "cat coverage/lcov.info | codecov",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover"
    }
  },
  "dependencies": {
    "material-ui": "^0.15.0-beta.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^2.8.0",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-watch": "^2.1.10",
    "ghooks": "^1.2.1",
    "mocha": "^2.4.5",
    "nyc": "^6.4.0",
    "react-addons-test-utils": "^15.0.1",
    "semantic-release": "^4.3.5",
    "webpack": "^1.13.0"
  },
  "private": true,
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
